{
	"extends": "@acme/tsconfig/base.json",
	"compilerOptions": {
		"lib": ["es2022", "dom", "dom.iterable"],
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"root/*": ["../../node_modules/*"]
		},
		"types": [
			"@cloudflare/workers-types/2023-07-01",
			"../../node_modules/@cloudflare/next-on-pages/dist/next-dev/index.d.ts",
			"react/canary"
		],
		"plugins": [{ "name": "next" }],
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
		"module": "esnext"
	},
	"include": [
		".",
		".next/types/**/*.ts",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"../../node_modules/@cloudflare/next-on-pages/dist/next-dev/index.d.ts"
	],
	"exclude": ["node_modules"]
}
